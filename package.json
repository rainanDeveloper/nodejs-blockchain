{
  "name": "nodejs_blockchain",
  "version": "1.0.0",
  "description": "A simple nodejs blockchain example",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main",
    "dev": "ts-node-dev src/main",
    "test": "npm run mocha",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "test.git"
  },
  "keywords": [
    "Nodejs",
    "Blockchain"
  ],
  "author": "Rainan Miranda de Jesus <rainan.jesus@pm.me>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
